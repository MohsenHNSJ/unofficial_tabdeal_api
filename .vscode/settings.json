{
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.extraPaths": [
        "${workspaceFolder}/"
    ],
    "python.analysis.diagnosticSeverityOverrides": {
        "reportAttributeAccessIssue": "none"
    },
    "flake8.args": [
        "--max-line-length=150"
    ],
    "pylint.args": [
        "--max-line-length=150",
        "--disable=fixme, unused-private-member, too-few-public-methods"
    ],
    "cSpell.words": [
        "aiohttp",
        "bdist",
        "BOMEUSDT",
        "BTCUSDT",
        "cpython",
        "dists",
        "etctabdeal",
        "HNSJ",
        "MANAUSDT",
        "Mohsen",
        "Mypy",
        "pycache",
        "Pylance",
        "Pylint",
        "pypa",
        "pypi",
        "sdist",
        "setuptools",
        "Sigstore",
        "Tabdeal",
        "testpypi"
    ]
}